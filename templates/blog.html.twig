{% extends 'base.html.twig' %}

{% block title %}My cool blog posts{% endblock %}

{% block body %}
    {% for entry in blog_entries %}
        <h2>{{ entry['title'] }}</h2>
        <p>{{ entry['body'] }}</p>
    {% endfor %}
{% endblock %}


{% block sidebar %}
    <h3>Table of Contents</h3>
    <a href="{{ url('blog_list', {'page': 1}) }}">aaaaa</a>

    {{ parent() }}
{% endblock %}